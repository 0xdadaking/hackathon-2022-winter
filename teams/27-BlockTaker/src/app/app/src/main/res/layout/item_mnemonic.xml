<?xml version="1.0" encoding="utf-8"?>
<com.csdn.roundview.RoundRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/mnemonic"
    android:layout_marginEnd="10dp"
    android:layout_marginBottom="10dp"
    app:rRadius="5dp"
    tools:context=".ui.channel.ImportActivity">
    <TextView
        android:id="@+id/tvNum"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:textSize="8sp"
        android:gravity="center"
        android:textColor="@color/white"
        tools:text="1"/>
    <TextView
        android:id="@+id/tvMnemonic"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="@color/primary_text_color"
        android:textSize="13sp"
        android:textStyle="bold"
        android:lines="1"
        tools:text="AAAAA"/>

</com.csdn.roundview.RoundRelativeLayout>